---
import Heading from "../ui/Heading.astro";
import Button from "../ui/Button.astro";
import { getCollection } from "astro:content";

const [staticData] = await getCollection("staticData");
const {
  contactSectionButtonText,
  email,
  contactSectionButtonIcon,
} = staticData.data;
---

<section>
  <div
    class="w-full bg-linear-to-b from-mint-100 to-transparent dark:from-mint-800 dark:border-mint-950 border-mint-100"
  >
    <div class="mx-auto max-w-5xl px-8 py-5 max-sm:py-12">
      <div
        class="flex items-center justify-center max-sm:flex-col max-sm:gap-8 max-sm:text-center"
      >
        <div
          class="transition-all duration-500 group-hover:animate-jump group-hover:animate-duration-[300ms] group-hover:animate-ease-in-out group-hover:scale-105 hover:scale-125"
        >
          
        </div>
      </div>
    </div>
  </div>
</section>
